{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@widgets/*": [
        "src/widgets/*"
      ],
      "@shared/*": [
        "src/shared/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
    }
  }
}